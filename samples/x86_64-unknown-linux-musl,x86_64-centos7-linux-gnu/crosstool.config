CT_CONFIG_VERSION="4"
CT_OBSOLETE=y
CT_ARCH_X86=y
CT_CANADIAN=y
CT_HOST="x86_64-unknown-linux-musl"
CT_ARCH_64=y
CT_TARGET_VENDOR="centos7"
CT_KERNEL_LINUX=y
CT_LINUX_V_3_10=y
# CT_KERNEL_LINUX_INSTALL_CHECK is not set
CT_GLIBC_V_2_17=y
# CT_CC_GCC_ENABLE_DEFAULT_PIE is not set
CT_CC_LANG_CXX=y
CT_BINUTILS_V_2_33=y
CT_STATIC_TOOLCHAIN=y

CT_EXPERIMENTAL=y
CT_TARBALL_RESULT=y
CT_TARBALL_RESULT_DIR="${CT_TOP_DIR}"
CT_TARBALL_RESULT_FILENAME="toolchain${CT_TOOLCHAIN_PKGVERSION:+-${CT_TOOLCHAIN_PKGVERSION}}-${CT_HOST:+HOST-${CT_HOST}-}${CT_TARGET}"
